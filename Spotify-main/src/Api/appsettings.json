{
  "ConnectionStrings": {
    "Featurify": "#{featurify-connectionstrings}"
  },
  "Authentication": {
    "Issuer": "#{issuer}",
    "Audience": "#{audience}",
    "SecretKey": "#{secretkey}",
    "DurationInMinutes": 0
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "SpotifyConfiguration": {
    "ClientId": "#{Spotify:ClientId}#",
    "ClientSecret": "#{Spotify:ClientSecret}#",
    "Endpoint": "#{SpotifyUrl}#"
  },
  "AllowedHosts": "*"
}
